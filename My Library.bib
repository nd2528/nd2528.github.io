	journal = {Movement Disorders: Official Journal of the Movement Disorder Society},
	author = {Alcalay, Roy N. and Mallett, Victoria and Vanderperre, Beno{\^i}t and Tavassoly, Omid and Dauvilliers, Yves and Wu, Richard Y. J. and Ruskey, Jennifer A. and Leblond, Claire S. and Ambalavanan, Amirthagowri and Laurent, Sandra B. and Spiegelman, Dan and Dionne-Laporte, Alexandre and Liong, Christopher and Levy, Oren A. and Fahn, Stanley and Waters, Cheryl and Kuo, Sheng-Han and Chung, Wendy K. and Ford, Blair and Marder, Karen S. and Kang, Un Jung and Hassin-Baer, Sharon and Greenbaum, Lior and Trempe, Jean-Francois and Wolf, Pavlina and Oliva, Petra and Zhang, Xiaokui Kate and Clark, Lorraine N. and Langlois, Melanie and Dion, Patrick A. and Fon, Edward A. and Dupre, Nicolas and Rouleau, Guy A. and Gan-Or, Ziv},
