}

@article{guo_resveratrol_2016,
	title = {Resveratrol alleviates {MPTP}-induced motor impairments and pathological changes by autophagic degradation of $\alpha$-synuclein via {SIRT1}-deacetylated {LC3}},
	volume = {60},
	issn = {1613-4133},
	doi = {10.1002/mnfr.201600111},
	abstract = {SCOPE: The accumulation of misfolded $\alpha$-synuclein in dopaminergic neurons is the leading cause of Parkinson's disease (PD). Resveratrol (RV), a polyphenolic compound derived from grapes and red wine, exerts a wide range of beneficial effects via activation of sirtuin 1 (SIRT1) and induction of vitagenes. Here, we assessed the role of RV in a 1-methyl-4-phenyl-1, 2, 3, 6-tetrahydropyridine (MPTP) induced mouse model of PD and explored its potential mechanisms.
METHODS AND RESULTS: RV and EX527, a specific inhibitor of SIRT1, were administered before and after MPTP treatment. RV protected against MPTP-induced loss of dopaminergic neurons, and decreases in tyrosine hydroxylase and dopamine levels, as well as behavioral impairments. Meanwhile, RV administration activated SIRT1. Microtubule-associated protein 1 light chain 3 (LC3) was then deacetylated and redistributed from the nucleus to the cytoplasm, which provoked the autophagic degradation of $\alpha$-synuclein in dopaminergic neurons. Furthermore, EX527 antagonized the neuroprotective effects of RV by reducing LC3 deacetylation and subsequent autophagic degradation of $\alpha$-synuclein.
CONCLUSION: We showed that RV ameliorated both motor deficits and pathological changes in MPTP-treated mice via activation of SIRT1 and subsequent LC3 deacetylation-mediated autophagic degradation of $\alpha$-synuclein. Our observations suggest that RV may be a potential prophylactic and/or therapeutic agent for PD.},
	language = {eng},
	number = {10},
	journal = {Molecular Nutrition \& Food Research},
	author = {Guo, Yan-Jie and Dong, Su-Yan and Cui, Xin-Xin and Feng, Ya and Liu, Te and Yin, Ming and Kuo, Sheng-Han and Tan, Eng-King and Zhao, Wen-Juan and Wu, Yun-Cheng},
	month = 10,
	year = {2016},
	pmid = {27296520},
	pmcid = {PMC6089356},
	keywords = {Animals, Autophagy, Male, Disease Models, Animal, Parkinson's disease, alpha-Synuclein, $\alpha$-synuclein, Neuroprotective Agents, Microtubule-Associated Proteins, Dopamine, Acetylation, Sirtuin 1, 1-Methyl-4-phenyl-1,2,3,6-tetrahydropyridine, Behavior, Animal, Corpus Striatum, LC3 deacetylation, Mice, Inbred C57BL, Parkinson Disease, Secondary, Resveratrol, SIRT1, Stilbenes},
