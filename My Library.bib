	keywords = {Humans, Cerebellum, Aged, Female, Male, Adult, Middle Aged, Spinocerebellar Ataxias, Disease Progression, Cerebellar Ataxia, Neurodegeneration, Ataxins, Spinocerebellar ataxias, Repressor Proteins, Subtypes, Trinucleotide Repeats},
