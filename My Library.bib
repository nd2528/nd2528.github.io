	pmid = {27235405},
	pmcid = {PMC4913434},
	keywords = {Humans, Cerebellum, Ataxia, Aged, Aged, 80 and over, Female, Male, Middle Aged, Multiple system atrophy, Mitochondrial Diseases, Cerebellar ataxia, Case-Control Studies, Oxidative Stress, Multiple System Atrophy, Ubiquinone, Coenzyme Q10, Muscle Weakness, Oxidative stress},
