	author = {Hua, Ping and Liu, Weiguo and Chen, Donghui and Zhao, Yanyan and Chen, Ling and Zhang, Ning and Wang, Chun and Guo, Suwan and Wang, Li and Xiao, Hong and Kuo, Sheng-Han},
